    "nextra-theme-docs": "^4.4.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@bobs-ui/eslint-config": "workspace:*",
    "@bobs-ui/typescript-config": "workspace:*",
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.3",
    "typescript": "5.9.2"
  }
